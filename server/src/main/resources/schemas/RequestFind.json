{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "find",
  "description": "find response",
  "type": "object",
  "properties": {
    "requestVersion": {
      "description":"the TIP protocol version",
      "type":"integer",
      "minimum": 1
    },
    "requestType": {
      "description":"the TIP object type should be find",
      "type":"string",
      "pattern":"^find$"
    },
    "match": {
      "description":"text string to match appropriate fields in the data source",
      "type":"string"
    },
    "limit": {
      "description":"maximum number of geographic locations to appear in response",
      "type":"integer"
    },
    "found": {
      "description":"total number of matching locations",
      "type":"integer"
    },
    "places": {
      "description":"list/array of objects describing the geographic locations",
      "type":"array"
    }
  },
  "required":["requestVersion","requestType"],
  "additionalProperties": false
}
